import terminals.bnf
import noqa_statement.bnf


<ident-list>
    ::= <ident>
    | <ident> <ident-list>

<block-indented>
    ::= <paragraph-indented> <split> <block-indented>
    | <paragraph-indented> <split-indented> <block-indented>
    | <paragraph-indented>

<split-indented>
    ::= <newline> <indents> <newline> <newlines>

<block>
    ::= <paragraph> <split> <block>
    | <paragraph>

<paragraph-indented>
    ::= <indented> <line> <newline> <paragraph-indented>
    | <indented> <line>

<paragraph>
    ::= <indents> <line> <newline> <paragraph>
    | <indents> <line>
    | <line> <newline> <paragraph>
    | <line>

<indented>
    ::= <indent> <indents>

<indents>
    ::= <indent> <indents>
    | ε

<split>
    ::= <newline> <newline> <newlines>

<newlines>
    ::= <newline> <newlines>
    | <newline>
    | ε

<line>
    ::= <word> <line>
    | <word> <noqa-maybe>
    | <noqa-statement>
